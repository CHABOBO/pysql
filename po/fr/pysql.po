msgstr ""
"(Certaines colonnes ont été tronquées pour convenir à la taille de votre "
"écran)"

#: src/pysql/pysqlshell.py:1454
msgid "Not connected to Oracle (Hello JF)"
msgstr "Non connecté à Oracle (salut JF !)"

#: src/pysql/pysqlshell.py:1469
msgid "Invalid argument. Use help <command name> for usage"
msgstr "Argument incorrect. Tapez « help <nom de la commande> » pour avoir de l'aide."

#: src/pysql/pysqlshell.py:1471
msgid "Invalid syntax for argument checking"
msgstr "Syntaxe incorrecte pour le contrôle des arguments"

#: src/pysql/pysqlshell.py:1496
msgid "Background query launched"
msgstr "Requête en arrière plan lancée"

#: src/pysql/pysqlshell.py:1516
msgid "Unknown output type !"
msgstr "Format de sortie inconnu !"

#: src/pysql/pysqlshell.py:1519
msgid " line(s) inserted"
msgstr " ligne(s) insérée(s)"

#: src/pysql/pysqlshell.py:1522
msgid " line(s) updated"
msgstr " ligne(s) mise(s) à jour"

#: src/pysql/pysqlshell.py:1525
msgid " line(s) deleted"
msgstr " ligne(s) supprimée(s)"

