msgid "employeeNumber select count(*) from employee"
msgstr "nombreEmployé select count(*) from employee"

#: src/pysql/pysqlshell.py:1562
msgid "recall a saved request:"
msgstr "Pour afficher toutes les requêtes"

#: src/pysql/pysqlshell.py:1563
msgid "employeNumber"
msgstr "nombreEmployé"

#: src/pysql/pysqlshell.py:1564
